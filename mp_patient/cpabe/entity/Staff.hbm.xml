<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="entity">

	<class name="cpabe.entity.Staff" table="tb_staff" >
		<id name="staff_id" column="staff_id" type="string">
		</id>
		
		<property name="name" column="name"/>
		<property name="password" column="password" />
		<property name="age" column="age" />
		<property name="work_age" column="work_age" />
		<property name="department" column="department" />
		<property name="duty" column="duty"/>	
		<property name="attr_1" column="attr_1"/>
		<property name="attr_2" column="attr_2"/>
		<property name="attr_3" column="attr_3"/>
		
	</class>	
</hibernate-mapping>
