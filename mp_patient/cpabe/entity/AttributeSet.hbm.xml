		<property name="attr_1" column="attr_1" />
		<property name="attr_2" column="attr_2" />
		<property name="attr_3" column="attr_3" />
		<property name="attr_4" column="attr_4" />
		<property name="attr_5" column="attr_5" />
		<property name="attr_6" column="attr_6" />
		<property name="attr_7" column="attr_7" />
		<property name="attr_8" column="attr_8" />
	</class>	
</hibernate-mapping>
